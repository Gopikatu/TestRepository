                    if('{!billingaddress.country}' != '') {
                        selected = '{!billingaddress.country}';
                    }
                } else {
                    if('{!billingaddress.country}' != ''){
                        selected = '{!shippingaddress.country}';
                    }
                }
                console.log(selected);
                country_array = [];
                
                contries.forEach(function(item, index) {
                    state_temp_array = [];
